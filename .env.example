# Mastercard PDF Bot - Configurações

# Credenciais Mastercard Connect
MASTERCARD_USER=seu-email@empresa.com
MASTERCARD_PASS=sua-senha-aqui

# Configurações do Bot
DOWNLOAD_LIMIT=5
DOWNLOAD_DIR=mastercard_pdfs

# Google Cloud (opcional)
GOOGLE_CLOUD_PROJECT=infinitepay-staging
GOOGLE_CLOUD_BUCKET=mastercard-pdf-bot-staging

# Configurações avançadas
HEADLESS_MODE=false
TIMEOUT_SECONDS=30
LOG_LEVEL=INFO
